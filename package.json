{
  "name": "yonui-mobile",
  "version": "0.4.267",
  "description": "yonui mobile",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "node --max_old_space_size=4096 ./node_modules/yonui-cli/bin start",
    "build": "rm -rf dist && node --max_old_space_size=4096 ./node_modules/yonui-cli/bin build -p",
    "dist": "rm -rf dist && node --max_old_space_size=4096 ./node_modules/yonui-cli/bin build dist -p",
    "assets": "copyfiles -e \"**/*.tsx\" \"./components/**/*\" lib",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build",
    "lib": "rm -rf lib && tsc && npm run assets",
    "test": "echo no test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:quiet": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --quiet --fix",
    "lint:git": "git diff --cached --name-only --diff-filter=ACM | grep '\\.[jt]sx\\?$' | xargs eslint",
    "sync": "npm run build -- -p && npm run move",
    "sync-dev": "npm run build && npm run move",
    "move": "cp -r dist/* ../viewer/public/componentLib && cp dist/manifest.json ../adapter/src/data/manifest.json",
    "move-n": "cp -r dist/* ../viewer/client/static/componentLib && cp dist/manifest.json ../adapter/src/data/manifest.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:git",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@git.yonyou.com:mdf-5.0/extension.git"
  },
  "files": [
    "README.md",
    "package.json",
    "lib",
    "dist",
    "components",
    "index.tsx",
    "style"
  ],
  "author": "Yudu Ban <banyd@yonyou.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-virtualized": "^9.21.9",
    "@types/shallowequal": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bee-icon": "^1.0.13",
    "bee-popover": "^3.1.0",
    "bee-radio": "^2.0.18",
    "bee-tag": "0.0.3",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-export": "^0.1.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.1",
    "highlight.js": "^9.18.1",
    "husky": "^3.1.0",
    "libraui-extension-packager": "^0.4.7",
    "marked": "^0.8.0",
    "qrcode.react": "^1.0.0",
    "react-highlight": "^0.12.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.2",
    "vconsole": "^3.3.4",
    "yonui-cli": "^0.1.68"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd-mobile": "^2.3.1",
    "classnames": "^2.2.1",
    "eslint-plugin-react": "^7.16.0",
    "moment": "^2.25.3",
    "mtl-js-sdk": "^5.0.6",
    "rc-rate": "^2.6.0",
    "rc-table": "~7.14.0",
    "react": "^16.12.0",
    "react-calendar": "^2.19.2",
    "react-dom": "^16.12.0",
    "react-virtualized": "^9.21.2",
    "rmc-feedback": "^2.0.0",
    "validator": "^13.1.1",
    "yonui-extension": "^0.7.32"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
